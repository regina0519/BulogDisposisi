{
  "expo": {
    "name": "Bulog Disposisi dan e-Verify Keuangan",
    "slug": "BulogDisposisi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.reginamondow.bulog"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Aplikasi Disposisi dan e-Verify Keuangan",
    "githubUrl": "https://github.com/regina0519/BulogDisposisi.git",
    "notification": {
      "icon": "./app/assets/notif-icon.png",
      "color": "#002C56"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./app/assets/notif-icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}